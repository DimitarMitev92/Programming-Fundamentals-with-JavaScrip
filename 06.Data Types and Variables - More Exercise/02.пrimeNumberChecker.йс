function primeNumber(num){
    let number = num;
    if((number / 2 === 1 || number / 3 === 1 || number / 5 === 1  || number / 7 === 1 || number / 11 === 1 || number / 1 === 1 || number / 13 === 1 || number / 17 === 1 || number / 19 === 1 || number/ 23 === 1 || number / 29 === 1 || number / 31 === 1 || number / 37 === 1 || number / 41 === 1 || number / 43 === 1 || number / 47 === 1 || number / 53 === 1 || number / 59 === 1 || number / 61 === 1 || number / 67 === 1 || number / 67 === 1 || number / 71 === 1 || number / 73 === 1 || number / 79 === 1 || number / 83 === 1 || number / 89 === 1 || number / 97 === 1) ){
        console.log("true")
    }else{
        console.log("false")
    }
}